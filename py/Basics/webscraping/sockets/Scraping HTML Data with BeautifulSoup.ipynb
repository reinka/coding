    "\n",
    "page = requests.get(input(\"Enter URL: \"), headers={\"User-Agent\":\"Mozilla/5.0\"})\n",
    "page.raise_for_status()\n",
    "soup = bs4.BeautifulSoup(page.text, \"lxml\")\n",
    "\n",
    "sum([int(span.contents[0]) for span in soup(\"span\")])"
