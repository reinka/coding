package de.mpaap.kurs1618;

public class Bruch {
    private int nenner;
    private int zaehler;
    
    @Override
    public String toString() {
        return this.zaehler + "/" + this.nenner;
    }
    
    public void setZaehler(int n) {
        this.zaehler = n;
    }
    public void setNenner(int n) {
        this.nenner = n;
    }
}
