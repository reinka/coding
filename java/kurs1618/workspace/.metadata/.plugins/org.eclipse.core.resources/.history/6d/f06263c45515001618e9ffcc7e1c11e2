package de.mpaap.kurs1618;

public class BruchTest {

    public static void main(String[] args) {
        Bruch r1 = new Bruch();
        r1.zaehler = 4;
        r1.nenner = 5;
        
        System.out.println("Zaehler:"+r1.zaehler);
        System.out.printf("Nenner: %d", r1.nenner);
    }
}
